{
  "name": "markdown-translate",
  "displayName": "markdown translate",
  "description": "markdown readme translate",
  "version": "0.0.3",
  "publisher": "markdown-translate",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.demo.testMenuShow",
        "title": "翻译当前文件",
        "icon": {
          "light": "./images/tool-light.svg",
          "dark": "./images/tool-light.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "editorFocus && resourceLangId == markdown",
          "command": "extension.demo.testMenuShow",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceLangId == markdown",
          "command": "extension.demo.testMenuShow",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/marked": "^0.6.5",
    "@types/request": "^2.48.1",
    "@types/urlencode": "^1.1.1",
    "extend": "^3.0.2",
    "marked": "^0.6.2",
    "psl": "^1.1.33",
    "request": "^2.88.0",
    "tough-cookie": "^3.0.1",
    "urlencode": "^1.1.0"
  },
  "__npminstall_done": false
}
